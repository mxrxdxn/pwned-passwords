  "keywords": ["password", "passwords", "security"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ron Marsden",
      "email": "hello@ronmarsden.com"
    },
    {
      "name": "Saif Eddin Gmati",
      "email": "azjezz2@gmail.com"
    }
  ],
  "autoload": {
    "psr-4" : {
      "PwnedPasswords\\": "src/PwnedPasswords/"
    }
  },
  "require": {
    "php": "^7.3 | ^8.0",
    "guzzlehttp/guzzle": "^6.3 | ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/var-dumper": "^5.3"
  }
}
